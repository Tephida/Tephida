{
  "name": "new",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "cssprod": "npx tailwindcss -i ./css/style.css -o ./dist/output.css --minify",
    "cssdev": "npx tailwindcss -i ./css/style.css -o ./dist/output.css --watch",
    "twinit": "npx tailwindcss init",
    "install": "npm install -D tailwindcss",
    "bootstrap": "npm update"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "postcss-nesting": "^10.1.3",
    "tailwindcss": "^3.0.23"
  }
}
